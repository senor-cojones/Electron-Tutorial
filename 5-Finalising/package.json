{
    "name": "electron-skeleton-app",
    "productName": "Skeleton",
    "version": "1.0.0",
    "description": "An introduction to Electron with an app skeleton",
    "main": "main.js",
    "scripts": {
        "clean": "rimraf Skeleton-*",
        "_build": "rimraf Skeleton-* && electron-packager . --platform=win32 --arch=x64 --icon=app",
        "build": "rimraf Skeleton-* && electron-packager . --platform=darwin --arch=x64 --icon=app",
        "start": "electron ."
    },
    "repository": "https://github.com/senor-cojones/Electron-Skeleton-App",
    "keywords": [
        "Electron",
        "introduction",
        "tutorial",
        "skeleton"
    ],
    "author": "senor-cojones",
    "devDependencies": {
        "electron": "~1.7.8",
        "electron-packager": "^9.1.0",
        "rimraf": "^2.6.2"
    }
}
